{
  "include": ["src/**/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "module": "preserve",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "rootDir": "src",
    "outDir": ".",
    "stripInternal": true,
    "tsBuildInfoFile": "./tsconfig.build.tsbuildinfo"
  }
}
